    "import matplotlib.pyplot as plt\n",
    "us_demographics = pd.read_csv('population_by_race.csv')\n",
    "us_demographics.columns = ['year','American Indian or Alaska Native', 'Asian Alone',\n",
    "       'Black or African American', 'Hispanic or Latino',\n",
    "       'Native Hawaiian or Pacific Islander', 'Some Other Race',\n",
    "       'Two or More Races', 'White Alone']\n",
    "# d.loc[0]\n",
    "pie_data = us_demographics.T.iloc[1:]\n",
    "pie_data = pie_data.drop([1,2,3,4,5,6,7,8],axis=1)\n",
    "pie_data = pie_data.drop(['Some Other Race','Two or More Races'],axis=0)\n",
    "\n",
    "pie_data = pie_data.reset_index()\n",
    "pie_data.columns = ['Race','Population']\n",
    "# Creating autocpt arguments\n",
    "def func(pct, allvalues):\n",
    "    absolute = int(pct / 100.*np.sum(allvalues))\n",
    "    return \"{:.1f}%\\n({:d} )\".format(pct, absolute)\n",
    "fig, ax = plt.subplots(figsize =(10, 7))\n",
    "explode = (0.1, 0.0, 0.2, 0.3, 0.0, 0.0)\n",
    "colors = ( \"pink\", \"orange\", \"brown\",\n",
    "          \"grey\", \"indigo\",'green')\n",
    "# Wedge properties\n",
    "wp = { 'linewidth' : 1, 'edgecolor' : \"green\" }\n",
    "wedges, texts, autotexts = ax.pie(pie_data.Population,\n",
    "                                  autopct = lambda pct: func(pct, pie_data.Population),\n",
    "                                  explode = explode,\n",
    "                                  labels = pie_data.Race,\n",
    "                                  shadow = True,\n",
    "                                  colors = colors,\n",
    "                                  startangle = 90,\n",
    "                                  wedgeprops = wp,\n",
    "                                  textprops = dict(color =\"black\"))\n",
    "# ax.pie(pie_data.Population,labels=pie_data.Race,autopct = lambda pct: func(pct, pie_data.Population))\n",
    "# Adding legend\n",
    "ax.legend(wedges, pie_data.Race,\n",
    "          title =\"Races\",\n",
    "          loc =\"center left\",\n",
    "          bbox_to_anchor =(1, 0, 0.5, 1))\n",
    " \n",
    "plt.setp(autotexts, size = 8, weight =\"bold\")\n",
    "ax.set_title(\"US POPULATION\")\n",
    " \n",
    "# show plot\n",
    "plt.show()"
   ]
